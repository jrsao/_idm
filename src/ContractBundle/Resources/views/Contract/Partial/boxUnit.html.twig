{% block box %}
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingOne">
            <h4 class="panel-title">
                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseUnitContract" aria-expanded="true" aria-controls="collapseUnitContract">
                    Contrats de locaux
                </a>
                <a href="{{ route }}"><button class="btn btn-default" ng-click="test()">+</button></a>
            </h4>
        </div>
        <div id="collapseUnitContract" class="panel-collapse collapse {% if display %} in {% endif %}" role="tabpanel" aria-labelledby="headingOne">
            <div class="panel-body">
                    {% embed "CommonBundle:Listing:listing.html.twig" with {'entities': unitContracts} %}
        
        {% block showRoute %}
            {{ path('contract_show', {'id': entity.id })}}
        {% endblock showRoute%}    
        
        {% block caption %}{% endblock caption%}
        
        {% block tableHeader %}
            {{ listing.header('Id')}}
            {{ listing.header('Local')}}
            {{ listing.header('Type de local')}}
            {{ listing.header('Prix')}}
            {{ listing.header('Client')}}
            {{ listing.header('Actif')}}
        {% endblock tableHeader%}
        
        {% block tableBody %}
            {{ listing.element( entity.id )}}
            {{ listing.element( entity.unit.no )}}
            {{ listing.element( entity.unit.unitType )}}
            {{ listing.element( entity.price )}}
            {{ listing.element( entity.client )}}
            {{ listing.element( 'oui' )}}
        {% endblock tableBody%}
        
    {% endembed %}
            </div>
        </div>
    </div>
{% endblock %} 