{% block box %}
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingOne">
            <h4 class="panel-title">
                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseParking" aria-expanded="true" aria-controls="collapseParking">
                    Stationnements
                </a>
                <a href="{{ route }}"><button class="btn btn-default" ng-click="test()">+</button></a>
            </h4>
        </div>
        <div id="collapseParking" class="panel-collapse collapse {% if display %} in {% endif %}" role="tabpanel" aria-labelledby="headingOne">
            <div class="panel-body">
                    {% embed "CommonBundle:Listing:listing.html.twig" with {'entities': parkings} %}
        
        {% block showRoute %}
{#            {{ path('parking_show', {'id': entity.id })}}#}
        {% endblock showRoute%}    
        
        {% block caption %}{% endblock caption%}
        
        {% block tableHeader %}
            {{ listing.header('Id')}}
            {{ listing.header('Numéro')}}
            {{ listing.header('Type de stationnement')}}
            {{ listing.header('Nombre de véhicules stationnés')}}
        {% endblock tableHeader %}
        
        {% block tableBody %}
            {{ listing.element( entity.id )}}
            {{ listing.element( entity.no )}}
            {{ listing.element( entity.parkingType.description )}}
            {{ listing.element( entity.vehicules|length )}}
        {% endblock tableBody%}
        
    {% endembed %}
            </div>
        </div>
    </div>
{% endblock %} 